{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "services/*": ["src/app/services/*"],
      "helpers/*": ["src/app/helpers/*"]
    },
    "module": "esnext",
    "target": "es5",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "lib": [
      "es2018",
      "dom"
     ]
  },
  "include": [
      "src"
  ]
}