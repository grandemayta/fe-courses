{
    "extends": [
      "tslint:latest",
      "tslint-config-prettier",
      "tslint-plugin-prettier"
    ],
    "rules": {
      "no-console": false,
      "no-implicit-dependencies": false,
      "no-submodule-imports": false,
      "prettier": [true, {
        "singleQuote": true 
      }]
    },
    "linterOptions": {
      "exclude": ["node_modules/**/*", "dist/**/*"]
    }
  }